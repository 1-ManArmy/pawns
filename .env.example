# OneLast AI - Environment Variables Configuration
# Copy this file to .env and fill in your actual API keys and configuration values

# =============================================================================
# CORE APPLICATION SETTINGS
# =============================================================================
VITE_APP_NAME="OneLast AI"
VITE_APP_VERSION="1.0.0"
VITE_APP_ENVIRONMENT="development"
VITE_APP_URL="https://onelastai.com"
VITE_APP_API_BASE_URL="https://api.onelastai.com/v1"

# =============================================================================
# AUTHENTICATION & SECURITY
# =============================================================================
VITE_JWT_SECRET="your-super-secure-jwt-secret-key-here"
VITE_ENCRYPTION_KEY="your-encryption-key-for-sensitive-data"
VITE_SESSION_TIMEOUT="3600"
VITE_MAX_LOGIN_ATTEMPTS="5"

# =============================================================================
# AI SERVICES - OPENAI GPT MODELS
# =============================================================================
VITE_OPENAI_API_KEY="sk-your-openai-api-key-here"
VITE_OPENAI_ORG_ID="org-your-organization-id"
VITE_OPENAI_MODEL_GPT4="gpt-4o"
VITE_OPENAI_MODEL_GPT35="gpt-3.5-turbo"
VITE_OPENAI_MAX_TOKENS="4000"
VITE_OPENAI_TEMPERATURE="0.7"

# =============================================================================
# AI SERVICES - ANTHROPIC CLAUDE
# =============================================================================
VITE_ANTHROPIC_API_KEY="sk-ant-your-anthropic-api-key-here"
VITE_ANTHROPIC_MODEL="claude-3-opus-20240229"
VITE_ANTHROPIC_MAX_TOKENS="4000"

# =============================================================================
# AI SERVICES - GOOGLE GEMINI
# =============================================================================
VITE_GOOGLE_API_KEY="your-google-ai-api-key-here"
VITE_GOOGLE_MODEL="gemini-pro"
VITE_GOOGLE_VISION_MODEL="gemini-pro-vision"

# =============================================================================
# AI SERVICES - COHERE
# =============================================================================
VITE_COHERE_API_KEY="your-cohere-api-key-here"
VITE_COHERE_MODEL="command-xlarge-nightly"

# =============================================================================
# AI SERVICES - HUGGING FACE
# =============================================================================
VITE_HUGGINGFACE_API_KEY="hf_your-huggingface-api-key-here"
VITE_HUGGINGFACE_MODEL="microsoft/DialoGPT-large"

# =============================================================================
# SPEECH & VOICE SERVICES
# =============================================================================
# ElevenLabs for Voice Generation
VITE_ELEVENLABS_API_KEY="your-elevenlabs-api-key-here"
VITE_ELEVENLABS_VOICE_ID="your-preferred-voice-id"

# Azure Speech Services
VITE_AZURE_SPEECH_KEY="your-azure-speech-service-key"
VITE_AZURE_SPEECH_REGION="your-azure-region"

# Google Speech-to-Text
VITE_GOOGLE_SPEECH_API_KEY="your-google-speech-api-key"

# =============================================================================
# IMAGE & VISION SERVICES
# =============================================================================
# DALL-E for Image Generation
VITE_DALLE_API_KEY="your-dalle-api-key-here"

# Stability AI for Image Generation
VITE_STABILITY_API_KEY="sk-your-stability-ai-api-key"

# Unsplash for Stock Images
VITE_UNSPLASH_ACCESS_KEY="your-unsplash-access-key"

# Cloudinary for Image Storage & Processing
VITE_CLOUDINARY_CLOUD_NAME="your-cloudinary-cloud-name"
VITE_CLOUDINARY_API_KEY="your-cloudinary-api-key"
VITE_CLOUDINARY_API_SECRET="your-cloudinary-api-secret"

# =============================================================================
# EMOTION & SENTIMENT ANALYSIS
# =============================================================================
# Azure Cognitive Services
VITE_AZURE_COGNITIVE_KEY="your-azure-cognitive-services-key"
VITE_AZURE_COGNITIVE_ENDPOINT="https://your-region.api.cognitive.microsoft.com"

# IBM Watson Sentiment Analysis
VITE_IBM_WATSON_API_KEY="your-ibm-watson-api-key"
VITE_IBM_WATSON_URL="your-ibm-watson-service-url"

# =============================================================================
# DOCUMENT & FILE PROCESSING
# =============================================================================
# PDF Processing Services
VITE_PDF_API_KEY="your-pdf-processing-api-key"

# Document AI Services
VITE_GOOGLE_DOCUMENT_AI_KEY="your-google-document-ai-key"

# =============================================================================
# DATABASE & STORAGE
# =============================================================================
# MongoDB Atlas
VITE_MONGODB_URI="mongodb+srv://username:password@cluster.mongodb.net/onelastai"

# Redis for Caching
VITE_REDIS_URL="redis://localhost:6379"
VITE_REDIS_PASSWORD="your-redis-password"

# AWS S3 for File Storage
VITE_AWS_ACCESS_KEY_ID="your-aws-access-key-id"
VITE_AWS_SECRET_ACCESS_KEY="your-aws-secret-access-key"
VITE_AWS_REGION="us-west-2"
VITE_AWS_S3_BUCKET="onelastai-files"

# =============================================================================
# THIRD-PARTY INTEGRATIONS
# =============================================================================
# GitHub Integration
VITE_GITHUB_CLIENT_ID="your-github-oauth-client-id"
VITE_GITHUB_CLIENT_SECRET="your-github-oauth-client-secret"

# Google OAuth
VITE_GOOGLE_CLIENT_ID="your-google-oauth-client-id"
VITE_GOOGLE_CLIENT_SECRET="your-google-oauth-client-secret"

# Discord Bot Integration
VITE_DISCORD_BOT_TOKEN="your-discord-bot-token"
VITE_DISCORD_CLIENT_ID="your-discord-client-id"

# Slack Integration
VITE_SLACK_BOT_TOKEN="xoxb-your-slack-bot-token"
VITE_SLACK_SIGNING_SECRET="your-slack-signing-secret"

# =============================================================================
# ANALYTICS & MONITORING
# =============================================================================
# Google Analytics
VITE_GA_TRACKING_ID="G-your-google-analytics-id"

# Sentry Error Tracking
VITE_SENTRY_DSN="your-sentry-dsn-url"

# LogRocket Session Recording
VITE_LOGROCKET_APP_ID="your-logrocket-app-id"

# =============================================================================
# PAYMENT & BILLING
# =============================================================================
# Stripe Payment Processing
VITE_STRIPE_PUBLISHABLE_KEY="pk_test_your-stripe-publishable-key"
VITE_STRIPE_SECRET_KEY="sk_test_your-stripe-secret-key"
VITE_STRIPE_WEBHOOK_SECRET="whsec_your-stripe-webhook-secret"

# PayPal Integration
VITE_PAYPAL_CLIENT_ID="your-paypal-client-id"
VITE_PAYPAL_CLIENT_SECRET="your-paypal-client-secret"

# =============================================================================
# EMAIL & COMMUNICATION
# =============================================================================
# SendGrid for Email
VITE_SENDGRID_API_KEY="SG.your-sendgrid-api-key"
VITE_SENDGRID_FROM_EMAIL="noreply@onelastai.com"

# Twilio for SMS
VITE_TWILIO_ACCOUNT_SID="your-twilio-account-sid"
VITE_TWILIO_AUTH_TOKEN="your-twilio-auth-token"
VITE_TWILIO_PHONE_NUMBER="+1234567890"

# =============================================================================
# SEARCH & KNOWLEDGE
# =============================================================================
# Algolia Search
VITE_ALGOLIA_APP_ID="your-algolia-app-id"
VITE_ALGOLIA_API_KEY="your-algolia-api-key"
VITE_ALGOLIA_SEARCH_KEY="your-algolia-search-only-key"

# Pinecone Vector Database
VITE_PINECONE_API_KEY="your-pinecone-api-key"
VITE_PINECONE_ENVIRONMENT="your-pinecone-environment"
VITE_PINECONE_INDEX_NAME="onelastai-vectors"

# =============================================================================
# GEOLOCATION & MAPPING
# =============================================================================
# Google Maps
VITE_GOOGLE_MAPS_API_KEY="your-google-maps-api-key"

# Mapbox
VITE_MAPBOX_ACCESS_TOKEN="pk.your-mapbox-access-token"

# =============================================================================
# SOCIAL MEDIA INTEGRATIONS
# =============================================================================
# Twitter API
VITE_TWITTER_API_KEY="your-twitter-api-key"
VITE_TWITTER_API_SECRET="your-twitter-api-secret"
VITE_TWITTER_BEARER_TOKEN="your-twitter-bearer-token"

# Facebook API
VITE_FACEBOOK_APP_ID="your-facebook-app-id"
VITE_FACEBOOK_APP_SECRET="your-facebook-app-secret"

# =============================================================================
# RATE LIMITING & QUOTAS
# =============================================================================
VITE_RATE_LIMIT_REQUESTS_PER_MINUTE="100"
VITE_RATE_LIMIT_REQUESTS_PER_HOUR="1000"
VITE_RATE_LIMIT_REQUESTS_PER_DAY="10000"

# =============================================================================
# DEVELOPMENT & TESTING
# =============================================================================
VITE_DEBUG_MODE="false"
VITE_MOCK_API="false"
VITE_ENABLE_ANALYTICS="true"
VITE_LOG_LEVEL="info"

# =============================================================================
# SECURITY & COMPLIANCE
# =============================================================================
VITE_CORS_ORIGINS="https://onelastai.com,https://www.onelastai.com"
VITE_ENABLE_HTTPS="true"
VITE_SESSION_SECURE="true"
VITE_COOKIE_SECURE="true"
VITE_CSRF_PROTECTION="true"